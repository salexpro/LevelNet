mixin case(icon, label, note)
    .column
        figure.use_cases_case
            .use_cases_case_top
                .use_cases_case_icon
                    img(src="./img/icons/" + icon + ".svg", alt=icon)
                if note
                    .use_cases_case_note
                        =note
            figcaption.use_cases_case_label
                !=label
section#use_cases.pattern.use_cases(data-magellan-target="use_cases")
    .row.align-center
        .column.large-10
            h2 Use Cases for Private Customers:
            h3 LevelNet End-Point Security
    .row.align-center.use_cases_videos
        .column.large-5
            a.use_cases_video(data-open="video")
                img(src="data:image/png;base64,", alt="")
        .column.large-5
            a.use_cases_video(data-open="video")
                img(src="data:image/png;base64,", alt="")
    .row.use_cases_cases
        +case('computer', 'LevelNet Endpoint Security App<br>with no other Antivirus Software')
        +case('security', 'LevelNet Endpoint Security App<br>with other Antivirus Software', 'Unable to detect a threat')
        +case('global', 'LevelNet Endpoint Security App<br>with other Antivirus Software', 'Able to detect a threat')