- var st_url = (page !== 'main' && page !== 'pre_ico' && page !== 'product') ? "./" : "";
nav.nav.is_filled
    .nav_inner(class=parent==='profile' ? 'row' : '')
        .logo.small-order-1
            a(href=st_url + "#top")
                img(src="./img/logo.svg", alt="LevelNet")
        if parent !== 'profile'
            ul.menu.show-for-xxlarge.small-order-2
                li
                    a(href="#" download) Whitepaper
                li
                    a(href=st_url + "#about") Technology
                li
                    a(href=st_url + "#team") Team
                if page != 'product'
                    li
                        a(href=st_url + "#ico") Token Sale
                li
                    a(href=st_url + "#press") Press
                li
                    a(href=st_url + "#faq") FAQ
            if page == 'product'
                a.button.hollow.filled.thin.show-for-xxlarge.small-order-3(href="/")
                    | Token Sale
            else
                button.button.hollow.filled.thin.show-for-xxlarge.small-order-3(data-open="join")
                    if page == 'pre_ico'
                        | Register for Token Sale
                    else
                        | Register for Whitelist
            .nav_phone.xxlarge-order-4.small-order-5
                a(href="tel:+823123456")
                    span +82 312 34 56
        else
            ul.menu.show-for-xxlarge.small-order-2
                li
                    a(href="./pre_ico.html") ICO Website
                li
                    a(href="./profile_settings.html") Account Settings
            .small-1.small-order-3
            .nav_profile.small-order-3
                .nav_profile_hex(style="background-image: url(https://placehold.it/200?text=S)")
                    .hex-top
                    .hex-bottom
                .nav_profile_user
                    | Sviatoslav
            ul.menu.small-order-4
                li
                    a(href="#") Log Out 
        .nav_lang.xxlarge-order-5.small-order-4
            select.select7.select7_dropdown.select7_remove_current
                option(value="EN") EN
                option(value="CN") CN
                option(value="SP") SP
                option(value="RU") RU
                option(value="GE") GE
                option(value="FR") FR
        button.hamb.hide-for-xxlarge.small-order-6(data-open="menu")