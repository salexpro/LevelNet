mixin review(large, is_big)
    .column(class="large-" + (large ? "6" : "3"))
        a.reviews_review(data-open="video")
            if is_big
                img(src="data:image/png;base64,", alt="")
            else
                img(src="data:image/png;base64,", alt="")

section#reviews.pattern.reviews(data-magellan-target="reviews")
    .row.align-center
        h2.column Video Reviews and Stories
    .row.reviews_inner
        +review(true, true)
        .column.large-6
            .row
                +review(true)
                +review(true)
                +review(true)
                +review(true)
        +review
        +review
        +review
        +review