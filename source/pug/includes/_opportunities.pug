mixin oppotunity
    figure.column.large-4.oppotunities_item
        a.oppotunities_item_img(href="#")
            img(src="data:image/png;base64,", alt="OKEX")
        figcaption.oppotunities_item_title
            a(href="#")
                | Visit exchange
                | 
                i.icon.icon--arrow

section.oppotunities
    .row
        .column.small-12
            h2 Get Wide Opportunities to Exchange
            h3 Q2 2018 LVL Token Listing on Exchanges
    .row.oppotunities_items
        - var i = 0
        while i < 9
            +oppotunity
            - i++